{
  "name": "genet",
  "productName": "genet",
  "version": "0.4.0",
  "description": "General Purpose Network Analyzer",
  "repository": {
    "type": "git",
    "url": "https://github.com/genet-app/genet.git"
  },
  "scripts": {
    "start": "node_modules/.bin/electron .",
    "build": "node scripts/build.js",
    "dist": "node_modules/.bin/electron-builder"
  },
  "main": "genet_modules/src/app.main.js",
  "author": "h2so5 <mail@h2so5.net>",
  "license": "MIT",
  "build": {
    "appId": "app.genet",
    "files": [
      "genet_modules",
      "package",
      "!package/**/target/**/*",
      "package/*/target/release/*.{dylib,dll,so}",
      "package/*/target/release/*{-cli,-cli.exe}"
    ],
    "asarUnpack": [
      "package",
      "genet_modules/bin"
    ],
    "mac": {
      "icon": "node_modules/@genet/image-assets/genet.icns",
      "category": "public.app-category.developer-tools"
    },
    "nsis": {
      "createDesktopShortcut": "always"
    },
    "dmg": {
      "background": "node_modules/@genet/image-assets/dmg-background.png",
      "icon": "node_modules/@genet/image-assets/genet-drive.icns",
      "iconSize": 128,
      "contents": [
        {
          "x": 450,
          "y": 180,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 180,
          "y": 185
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "rpm",
        "pacman"
      ],
      "category": "Network"
    },
    "deb": {
      "depends": [
        "libcap2-bin",
        "gconf-service"
      ]
    },
    "win": {
      "icon": "node_modules/@genet/image-assets/genet.ico"
    },
    "squirrelWindows": {
      "iconUrl": "https://raw.githubusercontent.com/genet-app/node_modules/@genet/image-assets/master/genet.ico",
      "loadingGif": "node_modules/@genet/image-assets/genet-install.gif"
    }
  },
  "dependencies": {
    "@genet/api": "0.2.2",
    "@genet/gpm": "0.0.10",
    "@genet/load-module": "0.0.3",
    "axios": "^0.17.1",
    "balloon-css": "^0.5.0",
    "bindings": "^1.3.0",
    "chokidar": "^2.0.4",
    "deep-equal": "^1.0.1",
    "disposables": "^1.0.2",
    "electron-squirrel-startup": "^1.0.0",
    "execa": "^1.0.0",
    "flat": "^4.1.0",
    "font-awesome": "^4.7.0",
    "fs-extra": "^7.0.0",
    "fuse.js": "^3.2.1",
    "glob": "^7.1.3",
    "js-yaml": "^3.12.0",
    "jsonschema": "^1.2.4",
    "lodash.debounce": "^4.0.8",
    "lodash.flatten": "^4.4.0",
    "lodash.throttle": "^4.1.1",
    "marked": "^0.3.19",
    "minimist": "^1.2.0",
    "mithril": "^1.1.1",
    "moment": "^2.20.0",
    "mousetrap": "^1.6.2",
    "object-path": "^0.11.4",
    "parse-color": "^1.0.0",
    "semver": "^5.5.1",
    "tempy": "^0.2.1",
    "title-case": "^2.1.1",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@genet/image-assets": "^0.1.0",
    "benchmark": "^2.1.4",
    "electron": "2.0.8",
    "electron-builder": "^20.28.4",
    "less": "^3.8.1",
    "nan": "^2.11.0",
    "node-gyp": "^3.8.0",
    "pretty-bytes": "^4.0.2",
    "ts-loader": "^4.5.0",
    "typedoc": "^0.11.1",
    "typedoc-plugin-ensure-internal": "git+https://github.com/deap82/typedoc-plugin-ensure-internal.git",
    "typescript": "^3.1.0-rc.20180911",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2"
  }
}
