{
    "compilerOptions": {
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "noImplicitAny": false,
        "strictNullChecks": true,
        "outDir": "./dist/",
        "module": "es6",
        "target": "es6",
        "allowJs": true
    },
    "typedocOptions": {
        "name": "genet",
        "out": "doc",
        "excludeExternals": true,
        "ignoreCompilerErrors": true,
        "mode": "file",
        "readme": "none"
    }
}